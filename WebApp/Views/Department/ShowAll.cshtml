@using Core.Models;
@{
    ViewData["Title"] = "Department Page";
}

<div class="text-center">
    <h1 class="display-4">AllEmployees</h1>
</div>

<table>
<tr>
    <th>Id</th>
    <th>Name</th>
    <th>Location</th>
</tr>
@{foreach (var item in ViewData["allDepartments"] as List<Department>){
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Location</td>
    </tr>
    }
}
</table>